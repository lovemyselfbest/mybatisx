node('master') {

    def appName = 'mybatisx'


  
    def workspace = sh (
                            script: 'cd ${WORKSPACE}@script/ && pwd', //script: 'cd ${WORKSPACE}@script/order/orderdata-host/ && pwd',
                            returnStdout: true
                       ).trim()


    dir(workspace){


        stage('Git'){
              sh " git branch:'1.0.0' url:'https://github.com/mybatisx/mybatisx.git'"
            }



        stage('Clean'){
                           sh "./gradlew :clean"
                        }

        stage('Jar'){
            
            sh "./gradlew :upload"
        }




    }

}
